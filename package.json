{
  "name": "objectify-folder",
  "version": "0.0.0-development",
  "description": "turn all non index.js modules in a folder as a single object",
  "main": "index.js",
  "scripts": {
    "test": "npm run test-js && npm run test-mjs",
    "test-js": "mocha -R spec --recursive",
    "test-mjs": "DEBUG=objectify-folder node --experimental-modules test-mjs/test.mjs"
  },
  "keywords": [
    "folder",
    "modules"
  ],
  "author": "mateodelnorte",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git@github.com:mateodelnorte/objectify-folder.git"
  },
  "bugs": {
    "url": "https://github.com/mateodelnorte/objectify-folder/issues"
  },
  "homepage": "https://github.com/mateodelnorte/objectify-folder",
  "dependencies": {
    "debug": "^4.1.0",
    "glob": "^7.1.2"
  },
  "devDependencies": {
    "assert": "1.4.1",
    "mocha": "6.1.2",
    "should": "13.2.3"
  }
}
